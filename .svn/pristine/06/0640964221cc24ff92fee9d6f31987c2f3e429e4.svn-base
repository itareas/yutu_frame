}
.login-submit{
    width: 320px;
    height:35px;
    line-height: 30px;
    background-color: #00C1DE;
    border-color: #00C1DE;
    margin: 8px 0;
    font-size:14px;
    font-weight: 700;
    text-align: center;
    color:#FFF;
    cursor: pointer;
    box-shadow: 0 1px 1px #EBE7E6;
}

@media (min-width: 1440px){
    .content{
        width: 100%;
        left: 0;
    }
    .navigate{
        left: 23%;
        width: 55%;
    }
    .navigate li{
        width: 30%;
        margin-right: 3%;
    }
}
@media (min-width: 1680px){
    .navigate{
        left: 25%;
        height: 130px;
        width: 51%;
    }
    .navigate li{
        width: 31%;
        padding-top: 15px;
        margin-right: 2%;
    }
}
