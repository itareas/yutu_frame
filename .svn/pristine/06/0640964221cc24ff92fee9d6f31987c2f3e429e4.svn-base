/*遮罩层*/
.dialog-cover{
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    opacity: 0.4;
    z-index: 200;
    background-color: #FFF;    background-color: rgba(0,0,0,.6);
    display: none;
}
/*登录样式*/
.login{
    position: absolute;
    top: 30px;
    right: 60px;
    z-index: 3;
}
.log{
    width: 60px;
    height: 30px;
    background-color: transparent;
    border:1px solid #FFF;
    color:#FFF;
    font-size: 14px;
}
.log:hover{
    color:#19BEF3;
    font-weight: bold;
    cursor: pointer;
    border-color: #089ACA;
}

/*登录框*/
.login-window{
    position: fixed;
    top: 50%;
    left: 50%;
    z-index: 250;
     width: 400px;
    height: 330px;
    margin-left: -175px;
    margin-top: -175px;
    display: none;
   
    margin-left: -210px;
    margin-top: -150px;
}

.login-dialog{
    position: absolute;
    top: 0;
    left: 0;
    width: 96%;
    height:96%;
