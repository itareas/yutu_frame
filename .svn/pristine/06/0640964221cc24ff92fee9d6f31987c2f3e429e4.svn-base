/*@charset="UTF-8";*/
body{
    width:100%;
    height: 100%;
    overflow: hidden;
}
.background{
    position: absolute;
    top: 0;
    bottom: 0;
    z-index: -1;
    overflow: hidden;
}
.background img{
    width: 100%;
    height: 100%;
}
.content{
    position: absolute;
    top: 15%;
    bottom: 0;
    width: 90%;
    left: 4%;
}
.content img{
    width: 100%;
}
.logo{
    position: absolute;
    top: 20px;
    left: 20px;
    z-index: 3;
    height: 36px;
    background: url(../images/home/logo.png) no-repeat;
    background-size: 36px 36px;
}
.logo span{
    display: block;
    line-height: 36px;
    margin-left: 40px;
    font-size: 20px;
}
/*右侧用户信息及退出*/
.right{
    position: absolute;
    top: 30px;
    right: 30px;
    z-index: 3;
    height: 27px;
    line-height: 27px;
    display: none;
}
.name{
    float:left;
    position: relative;
    margin-right: 20px;
    height: 27px;
    border: 1px solid #1053A3;
    border-radius: 20px;
}
.admin-icon{
    float: left;
    width: 27px;
    height: 27px;
    margin-right: 10px;
    border: 1px solid #1053A3;
    border-radius: 27px;
    background: url(../images/common/user.png) 4px 4px no-repeat;
    cursor: pointer;
}
.user{
    float: left;
    height: 100%;
    line-height: 25px;
    font-size:14px;
    color: #87D3FF;
}
.user:hover{
    color:#FFF;
}
.user span{
    overflow: hidden;
}

.edit {
    position: absolute;
    top: 28px;
    left: 15px;
    display: none;
    border-radius: 4px;
}
.edit ul {
    font-size: 14px;
    background: #023E67;
    padding: 5px 0 6px 0;
}
.edit ul li {
    width: 100px;
    margin: 2px 0;
}
.edit ul li:hover {
    background-color: #02497A;
    cursor: pointer;
}
.edit ul li:hover a {
    color: #FFF;
}
.edit ul li a {
    width: 80px;
    height: 25px;
    line-height: 25px;
    display: block;
    margin-left: 22px;
    color: #87D3FF;
}
 
.exit{
    width: 50px;
    height: 100%;
    color: #87D3FF;
    font-size:14px;
    float:left;
}
.exit:hover{
    color:#FFF;
}
/* 二级标题 */
.title{
	position: absolute;
    top: 14%;
    width: 100%;
    text-align: center;
    color: #FFF;
    font-size: 26px;
}
/*其他功能页面链接*/
.navigate{
    position: absolute;
    top: 28%;
    left: 24%;
    z-index: 3;
    width: 52%;
    min-width: 670px;
    height: 120px;
}
.navigate ul{
    height: 100%;
}
.navigate li{
    position: relative;
    width: 31%;
    height: 100%;
    float: left;
    margin-right: 2%;
    margin-bottom: 20px;
    padding-top: 10px;
    /*图片在div中居中*/
    display: table-cell;
    text-align: center;
    color: #FFF;
    border-radius: 15px;
    opacity: 0.95;
}
.navigate li img{
    width: 60px;
    height: 60px;
    -webkit-transition:all .5s; /* Safari 和 Chrome */
    -moz-transition:all .5s; /* Firefox */
    -o-transition:all .5s; /* Opera */
    transition:all .5s;
}
.navigate li p{
    font-size: 17px;
    margin: 0 2px;
}
li.vertical p{
    margin-top: 10px;
} 
.navigate li:hover:not(.no-pro){
    cursor: pointer;
}
.navigate li:hover:not(.no-pro) img{
    -webkit-transform:rotateY(180deg);
    -moz-transform:rotateY(180deg);
    -o-transform: rotateY(180deg);
    transform: rotateY(180deg);
}

.navigate li.no-pro:after{
	content:"";
	display:block;
	width:100%;
	height:100%;
	position:absolute;
	top:0;
	left:0;
	background:gray;
	opacity:0.7;
    border-radius: 15px;
}
.sjhj{
    background: -webkit-linear-gradient(#4b92fc, #2568cb); /* Safari 5.1 - 6.0 */
    background: -o-linear-gradient(#4b92fc, #2568cb); /* Opera 11.1 - 12.0 */
    background: -moz-linear-gradient(#4b92fc, #2568cb); /* Firefox 3.6 - 15 */
    background: linear-gradient(#4b92fc, #2568cb); /* 标准的语法 */
}
.sjhj:hover{
    background: #2568cb;
}
.yzt{
    background: -webkit-linear-gradient(#cb7ce0, #ab50c3);
    background: -o-linear-gradient(#cb7ce0, #ab50c3);
    background: -moz-linear-gradient(#cb7ce0, #ab50c3);
    background: linear-gradient(#cb7ce0, #ab50c3);
}
.yzt:hover{
    background: #ab50c3;
}
.hbyq{
    background: -webkit-linear-gradient(#38b9bd, #07979b);
    background: -o-linear-gradient(#38b9bd, #07979b);
    background: -moz-linear-gradient(#38b9bd, #07979b);
    background: linear-gradient(#38b9bd, #07979b);
}
.hbyq:hover{
    background: #07979b;
}
.lsgyl{
    background: -webkit-linear-gradient(#e0cc7c, #bf9c46);
    background: -o-linear-gradient(#e0cc7c, #bf9c46);
    background: -moz-linear-gradient(#e0cc7c, #bf9c46);
    background: linear-gradient(#e0cc7c, #bf9c46);
}
.lsgyl:hover{
    background: #bf9c46;
}
.lsslsz{
    background: -webkit-linear-gradient(#30baf6, #0193d3);
    background: -o-linear-gradient(#30baf6, #0193d3);
    background: -moz-linear-gradient(#30baf6, #0193d3);
    background: linear-gradient(#30baf6, #0193d3);
}
.lsslsz:hover{
    background: #0193d3;
}
.hbcy{
    background: -webkit-linear-gradient(#7269eb, #6841e8);
    background: -o-linear-gradient(#7269eb, #6841e8);
    background: -moz-linear-gradient(#7269eb, #6841e8);
    background: linear-gradient(#7269eb, #6841e8);
