}
.verifycode{
    height: 35px;
    margin: 20px 0;  
    position: relative;
}
.verifycode input{	
	width: 160px;
	height:35px;
	line-height:35px;
	position: absolute;
	left:0px;
	top:0px;
    padding-left:5px;    
    border:1px solid #D7D8D9;
}
.verifycode img {
    position: absolute;
	left:170px;
	top:0px;
    width: 120px;
    height: 35px;
    border:0;
   }
.captchaImage{
	 margin: 10px 0 8px;   
}

.username img, .password img{
    position: absolute;
